  "scripts": {
    "backup": "git add . && git commit -a -m backup && git push",
    "build": "npm run clean && npm run lint && node .config/build.js",
    "clean": "rm -rf dist .nyc_output coverage; mkdir dist",
    "deploy": "node .config/deploy.js",
    "dev": "sudo PORT=80 node .config/dev-server.js",
    "e2e": "node test/e2e/runner.js",
    "lint": "eslint --ext .js,.vue src .config .script test/*_spec.js",
    "prepublish": "npm run build",
    "publish": "npm run scp && npm run backup &",
    "scp": "sh .script/publish.sh",
    "start": "npm run dev",
    "test": "npm run unit && npm run e2e",
    "unit": "karma start test/unit/karma.conf.js --single-run"
  },
  "config": {
    "ghooks": {
      "pre-commit": "NODE_ENV=production node .script/checkVersion.js && npm run lint",
      "pre-push": "git status"
    }
